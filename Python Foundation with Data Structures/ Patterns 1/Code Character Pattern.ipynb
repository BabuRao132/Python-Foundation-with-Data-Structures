    "## Read input as specified in the question\n",
    "## Print the required output in given format\n",
    "n=int(input())\n",
    "i=1\n",
    "while i<=n:\n",
    "    j=1\n",
    "    start=chr(ord('A')+i-1)\n",
    "    while j<=i:\n",
    "        charp=chr(ord(start)+j-1)\n",
    "        print(charp,end=\"\")\n",
    "        j=j+1\n",
    "    print()\n",
    "    i=i+1\n",
    "\n",
